{
  "name": "automation-testing-with-puppeteer",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/mocha/bin/mocha --timeout=30000 tests",
    "test:device": "./node_modules/mocha/bin/mocha --timeout=30000 tests/device.test.js",
    "test:e2e": "./node_modules/mocha/bin/mocha --timeout=30000 puppeteer-functionally/tests/e2e/*.test.js",
    "test:snapshot": "jest --config=jest.config.js --detectedOpenHandles --forceExit",
    "test:snapshot:update": "jest --config=jest.config.js --updateSnapshot --detectOpenHandle --forceExit"
  },
  "keywords": [
    "Puppeteer",
    "Mocha",
    "Chai",
    "Automation"
  ],
  "author": "Alexander Shulha (sashamiller666@gmail.com)",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SashaJson/automated-software-testing-with-puppeteer.git"
  },
  "bugs": {
    "url": "https://github.com/SashaJson/automated-software-testing-with-puppeteer/issues"
  },
  "homepage": "https://github.com/SashaJson/automated-software-testing-with-puppeteer#readme",
  "description": "Automation testing with puppeteer",
  "dependencies": {
    "chai": "^4.2.0",
    "jest": "^26.6.3",
    "jest-image-snapshot": "^4.3.0",
    "mocha": "^8.2.1",
    "puppeteer": "^5.5.0"
  }
}
